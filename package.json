{
  "name": "activity-rank-bot",
  "version": "5.0.0",
  "description": "ActivityRank Bot",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "production": "NODE_ENV=production node ./src/app.js",
    "development": "NODE_ENV=development node ./src/app.js",
    "developmentNodemon": "nodemon --exec \"npm run build && npm run dev\"",
    "typecheck": "tsc --noEmit",
    "build": "swc src --out-dir dist --copy-files",
    "dev": "node --enable-source-maps ./dist/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Linck01/activityRankBot.git"
  },
  "keywords": [
    "discord",
    "bot",
    "rank"
  ],
  "dependencies": {
    "@discordjs/rest": "^0.2.0-canary.0",
    "@sapphire/duration": "^1.0.0",
    "@sapphire/snowflake": "^3.2.2",
    "common-tags": "^1.8.2",
    "discord.js": "^14.7.0",
    "node-cron": "^3.0.2",
    "node-emoji": "^1.11.0",
    "pino": "^8.8.0",
    "pretty-ms": "^7.0.1",
    "promise-mysql": "^5.0.4",
    "public-ip": "^4.0.4"
  },
  "author": "Linck",
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.80",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.14",
    "pino-pretty": "^9.1.1",
    "typescript": "^5.2.2"
  },
  "license": "",
  "nodemonConfig": {
    "delay": 2500,
    "ext": "js,ts",
    "watch": [
      "src/**/*.ts"
    ]
  }
}
