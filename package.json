{
  "name": "activity-rank-bot",
  "version": "5.0.0",
  "description": "ActivityRank Bot",
  "exports": "./src/app.js",
  "type": "module",
  "scripts": {
    "production": "NODE_ENV=production node ./src/app.js",
    "development": "NODE_ENV=development node ./src/app.js",
    "developmentNodemon": "nodemon --exec \"npm run build && npm run dev\"",
    "format": "prettier -w src",
    "typecheck": "tsc --noEmit",
    "build": "swc src --out-dir dist --copy-files",
    "dev": "node --enable-source-maps ./dist/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Linck01/activityRankBot.git"
  },
  "keywords": [
    "discord",
    "bot",
    "rank"
  ],
  "dependencies": {
    "@discordjs/rest": "^2.2.0",
    "@sapphire/duration": "^1.1.0",
    "@sapphire/snowflake": "^3.5.1",
    "callsites": "^4.1.0",
    "common-tags": "^1.8.2",
    "discord.js": "^14.14.1",
    "glob": "^10.3.10",
    "node-cron": "^3.0.3",
    "node-emoji": "^2.1.1",
    "pino": "^8.16.2",
    "pretty-ms": "^8.0.0",
    "promise-mysql": "^5.2.0",
    "public-ip": "^6.0.1"
  },
  "author": "Linck",
  "devDependencies": {
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.96",
    "@types/common-tags": "^1.8.4",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "commander": "^11.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "nodemon": "^3.0.1",
    "pino-pretty": "^10.2.3",
    "prettier": "^3.1.0",
    "typescript": "^5.2.2"
  },
  "license": "",
  "nodemonConfig": {
    "delay": 2500,
    "ext": "js,ts",
    "watch": [
      "src/**/*.ts"
    ]
  }
}
